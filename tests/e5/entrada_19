funcao_extrema => int
{
    int x <= 10;
    int result <= 0;

    if (x > 0) {
        if (x < 2) {
            if (x > 1) {
                while (x > 0) {
                    x = x - 1;
                    result = result + 1;
                    while (result < 10) {
                        int a <= 1;
                        while (a > 0) {
                            a = a - 1;
                            result = result + 1;
                        };
                    };
                };
            };
        };
    };

    return result;
}
